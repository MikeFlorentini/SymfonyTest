<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
            <link rel="stylesheet" href="{{ asset('/css/main.css') }}"
        {% endblock %}
    </head>
    <body>
        

        <div class="row">
            <div class="col-sm-2">
                <div id="mySidenav" class="sidenav">
                    <a href="{{ path('app_homepage', {slug: 'DelCatMaker'}) }}"><img src="{{ asset('/img/homeicon.svg') }} " class="img-rounded" id="logo"></a>
                    <a href="{{ path('app_delcatmaker') }}">DelCatMaker</a>
                    <a href="{{ path('app_course_export') }}">CourseExport</a>
                </div>
            </div>
            <div class="col-sm-10">
                {% block body %}{% endblock %}
            </div>
        </div>
        
        {% block javascripts %}
            <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
        {% endblock %}
    </body>
</html>


<!-- { asset('css pfad ') }} so css dateien einbinden im href -->